<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expense_Approver_Manager_VR</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  ISPICKVAL( Type__c ,&quot;Expense(Outgoing)&quot;) ,
 NOT( 
Approver__c  =  $User.ManagerId
)
)</errorConditionFormula>
    <errorMessage>If Type is Expense(Outgoing) Approver Should be your Manager</errorMessage>
</ValidationRule>
